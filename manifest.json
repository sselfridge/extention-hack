{
  "name": "Chill Out Refresh",
  "version": "1.1",
  "description": "Build an Extension!",
  "permissions": [
    "declarativeContent",
    "storage",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "commands": {
    "toggle-refresh": {
      "suggested_key": {
        "default": "Alt+S",
        "mac": "Alt+S"
      },
      "description": "Toggle Page Refresh"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "file:///*html"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}